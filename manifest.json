
{
  "name":"canvas",
  "version":"0.0.2",
  "manifest_version":3,
  "description":"tool",
  "permissions":[
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css" : ["canvasext.css"]
    }
  ]
}